{
    "name": "ringierimu/event-bus",
    "description": "Send Laravel Events to Event Bus",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/RingierIMU/laravel-event-bus/issues",
        "source": "https://github.com/RingierIMU/laravel-event-bus"
    },
    "keywords": ["laravel", "events", "service", "bus"],
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percym@ringier.co.za",
            "role": "Developer"
        },
        {
            "name": "Norby Baruani",
            "email": "norbyb@ringier.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ringierimu\\EventBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ringierimu\\EventBus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": [
            "Ringierimu\\EventBus\\EventBusServiceProvider"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
